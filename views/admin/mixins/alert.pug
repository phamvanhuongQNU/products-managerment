mixin alert-success(time)
  - if (messages.success)
    .message.success
      span(class="alert alert-success",data-time=time,show-alert) #{messages.success}
  
mixin alert-error(time)
  - if (messages.error)
    .message.success
      span(class="alert alert-danger",data-time=time,show-alert) #{messages.error}
    